record(longout, "$(PREFIX):iPSUOffReqRaw") {
    field(DESC, "Power supply off request register (raw)")
    field(DTYP, "ecat2")
    field(OUT, "@s$(SLAVE_ID).sm$(SYNC_MAN).p0.e2.Tuint16")
