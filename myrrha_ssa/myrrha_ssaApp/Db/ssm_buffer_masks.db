# bi and bo records referencing bits of buffer download trigger mask.
# They are congruent with SSM status bits.

record(longin, "$(PREFIX)-$(SSM_NUM):SSMStatTrigMask-Sts") {
    field(DESC, "SSM status trigger mask state")
    field(SCAN, "I/O Intr")
    field(DTYP, "asynUInt32Digital")
    field(INP, "@asynMask($(BFR_PORT),0,0xffff)SSMStat")
    field(PINI, "YES")
}

