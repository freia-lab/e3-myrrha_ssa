# The auxilliary Modbus data frame.

record(ai, "$(PREFIX)-$(SSM_NUM):Pall1Vswr") {
    field(DESC, "Pallet voltage standing wave ratio")
    field(SCAN, "I/O Intr")
    field(DTYP, "asynFloat64")
    field(INP, "@asyn($(PORT),0)FLOAT32_BE")
    field(PREC, "2")
}

