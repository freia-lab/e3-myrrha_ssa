# SSA command registers written via EtherCAT.
# The ecat2 module does not support mbboDirect records, so data
# is forwarded from a soft mbboDirect and written by a longout.
# Each longout-mbboDirect pair also has a seq record that clears
# the register to 0.

record(mbboDirect, "$(PREFIX):iPSUOnReq") {
    field(DESC, "Power supply on request register")
    field(OUT, "$(PREFIX):iPSUOnReqRaw PP")
    field(FLNK, "$(PREFIX):iPSUOnReqClr")
}

record(longout, "$(PREFIX):iPSUOnReqRaw") {
    field(DESC, "Power supply on request register (raw)")
    field(DTYP, "ecat2")
    field(OUT, "@s$(SLAVE_ID).sm$(SYNC_MAN).p$(PDO).e0.Tuint16")
}

record(seq, "$(PREFIX):iPSUOnReqClr") {
    field(DESC, "Clears iPSUOnReq after command")
    field(DOL1, "0")
    field(DLY1, "0.1")
    field(LNK1, "$(PREFIX):iPSUOnReq PP")
}

record(mbboDirect, "$(PREFIX):iPSUOffReq") {
    field(DESC, "Power supply off request register")
    field(OUT, "$(PREFIX):iPSUOffReqRaw PP")
    field(FLNK, "$(PREFIX):iPSUOffReqClr")
}

record(longout, "$(PREFIX):iPSUOffReqRaw") {
    field(DESC, "Power supply off request register (raw)")
